<main>
  {{#if isRegistation}}
    {{> registration}}
  {{else}} 
    {{#if isAuth}}
      {{> authorization}}
    {{else}}
      {{#if isProfile}}
        {{> profilePage}}
      {{/if}}
      {{#if isReception}}
        {{> reception}}
      {{/if}}
      {{#if isCalendar}}
        {{> calendar}}
      {{/if}}
      {{#if isMess}}
        {{> messenger}}
      {{/if}}
      {{#if isPayment}}
        {{> payment}}
      {{/if}}
    {{/if}}
  {{/if}}
</main>